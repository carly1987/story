{
  "productName": "story",
  "version": "0.1.0",
  "identifier": "com.story.appstory",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "story",
        "width": 1600,
        "height": 1200,
        "transparent": true
      }
    ],
    "security": {
      "csp": "default-src 'self' data: blob: 'unsafe-inline' 'unsafe-eval'"
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "iOS": {
      "minimumSystemVersion": "13.0",
      "developmentTeam": "Jared Griffin (ID: 54T34FM7VV)"
    },
    "android": {
      "minSdkVersion": 28
    }
  }
}
